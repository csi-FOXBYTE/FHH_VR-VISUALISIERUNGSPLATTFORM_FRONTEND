import "common"
import "event"
import "layer"
import "permissions"
import "user"
import "config"

plugin prisma {
  provider = '@core/prisma'
  output = '../prisma/schema.prisma'
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
