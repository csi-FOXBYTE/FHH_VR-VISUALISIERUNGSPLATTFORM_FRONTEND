{
  "name": "fhhvr-webfrontend",
  "version": "0.0.15",
  "private": true,
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "prisma": {
    "seed": "tsx prisma/scripts/seed.ts"
  },
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "docker compose up -d && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "postinstall": "node ./scripts/copyCesium.mjs",
    "lint": "next lint",
    "prepare": "husky",
    "generate-gateway-api": "pnpm rimraf ./src/server/gatewayApi/generated && pnpm openapi-generator-cli generate -i http://localhost:5000/docs/json -g typescript-fetch -o ./src/server/gatewayApi/generated",
    "zenstack-generate": "pnpm zenstack generate --schema zmodel/schema.zmodel",
    "preinstall": "npx only-allow pnpm",
    "db:reset": "pnpm zenstack generate --schema zmodel/schema.zmodel && pnpm tsx src/server/prisma/extensions/resetSystemVersionedTables.ts && pnpm prisma db push --force-reset",
    "db:eject": "pnpm prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script > ./prisma/migration.sql",
    "publish:docker": "docker build -t fhhvr-webfrontend:0.0.15 . --secret id=npmrc,src=%USERPROFILE%/.npmrc --secret id=env,src=.env && docker tag fhhvr-webfrontend:0.0.15 fhhvrdockerregistry.azurecr.io/fhhvr-webfrontend:0.0.15 && docker push fhhvrdockerregistry.azurecr.io/fhhvr-webfrontend:0.0.15"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@azure/storage-blob": "^12.27.0",
    "@csi-foxbyte/prisma-mssql-realtime-extension": "^0.0.5",
    "@emotion/cache": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.13.5",
    "@faker-js/faker": "^9.3.0",
    "@mui/icons-material": "^6.1.9",
    "@mui/lab": "7.0.0-beta.12",
    "@mui/material": "^7.1.1",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/x-data-grid": "^7.23.0",
    "@mui/x-date-pickers": "^8.3.1",
    "@mui/x-tree-view": "^8.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@openapitools/openapi-generator-cli": "^2.21.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.56.0",
    "@opentelemetry/sdk-logs": "^0.56.0",
    "@prisma/client": "^6.2.1",
    "@tanstack/react-query": "^5.59.20",
    "@trpc/client": "11.4.3",
    "@trpc/next": "11.4.3",
    "@trpc/react-query": "11.4.3",
    "@trpc/server": "11.4.3",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/mssql": "^9.1.7",
    "@types/proj4": "^2.5.6",
    "@types/react-big-calendar": "^1.16.2",
    "@vercel/otel": "^1.10.0",
    "@zenstackhq/runtime": "2.14.2",
    "@zenstackhq/sdk": "^2.16.0",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.10.0",
    "bullmq": "^5.56.1",
    "cesium": "^1.129.0",
    "client-only": "^0.0.1",
    "dayjs": "^1.11.13",
    "exceljs": "^4.4.0",
    "jose": "^6.0.11",
    "matcher": "^5.0.0",
    "material-ui-confirm": "^4.0.0",
    "material-ui-popup-state": "^5.3.6",
    "next": "15.4.3",
    "next-absolute-url": "^1.2.2",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "^4.3.4",
    "notistack": "^3.0.2",
    "nuqs": "^2.2.1",
    "pkce-challenge": "^5.0.0",
    "prisma": "^6.2.1",
    "proj4": "^2.17.0",
    "proj4-list": "^1.0.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "react-hook-form-mui": "^7.6.2",
    "resium": "1.19.0-beta.1",
    "rxjs": "^7.8.1",
    "rxjs-for-await": "^1.0.0",
    "sass": "^1.89.2",
    "server-only": "^0.0.1",
    "sharp": "^0.34.2",
    "superjson": "^2.2.1",
    "tsx": "^4.19.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^20",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@zenstackhq/trpc": "^2.16.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "15.4.3",
    "husky": "^9.1.7",
    "typescript": "^5",
    "zenstack": "2.14.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.8",
      "@types/react-dom": "19.1.6"
    }
  }
}
