datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                      String            @id() @default(cuid())
  createdAt               DateTime          @default(now())
  updatedAt               DateTime          @updatedAt()
  name                    String?
  phoneNumber             String?
  email                   String            @unique()
  emailVerified           DateTime?
  image                   String?
  accounts                Account[]
  sessions                Session[]
  Authenticator           Authenticator[]
  projects                Project[]
  attendedEvents          EventAttendee[]
  createdEvents           Event[]
  createdBaseLayers       BaseLayer[]
  createdProjectLayers    ProjectLayer[]
  createdVisualAxes       VisualAxis[]
  createdStartingPoints   StartingPoint[]
  createdClippingPolygons ClippingPolygon[]
  visibleProjects         Project[]         @relation("visibleForUsers")
  assignedGroups          Group[]
}

model Account {
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String?  @db.Text()
  access_token      String?  @db.Text()
  expires_at        Int?
  token_type        String?  @db.Text()
  scope             String?  @db.Text()
  id_token          String?  @db.Text()
  session_state     String?  @db.Text()
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt()
  user              User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([provider, providerAccountId])
}

model Session {
  sessionToken String   @unique()
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt()
}

model VerificationToken {
  identifier String
  token      String
  expires    DateTime

  @@id([identifier, token])
}

model Authenticator {
  credentialID         String  @unique()
  userId               String
  providerAccountId    String
  credentialPublicKey  String  @db.Text()
  counter              Int
  credentialDeviceType String
  credentialBackedUp   Boolean
  transports           String? @db.Text()
  user                 User    @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([userId, credentialID])
}

model Project {
  owner                 User?             @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId               String?
  id                    String            @id() @default(cuid())
  createdAt             DateTime          @default(now())
  updatedAt             DateTime          @updatedAt()
  title                 String            @db.Text()
  description           String            @db.Text()
  visibleForGroups      Group[]
  visibleForUsers       User[]            @relation("visibleForUsers")
  startingPoints        StartingPoint[]
  clippingPolygons      ClippingPolygon[]
  includedBaseLayers    BaseLayer[]
  includedProjectLayers ProjectLayer[]    @relation("includedInProjects")
  projectLayers         ProjectLayer[]
  events                Event[]
}

model ClippingPolygon {
  owner     User?    @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId   String?
  id        String   @id() @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()
  name      String
  /// Comma separated point list [x0, y0, z0, ..., xn, yn, zn]
  points    String   @db.Text()
  project   Project  @relation(fields: [projectId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  projectId String
}

model ProjectFile {
  id             String       @id() @default(cuid())
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt()
  name           String
  href           String
  /// JSON object of all assigned meta data
  attributes     String       @db.Text()
  projectLayer   ProjectLayer @relation(fields: [projectLayerId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  projectLayerId String
}

/// Is global per definition, can not be associatd to a project.
model VisualAxis {
  owner       User?    @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId     String?
  id          String   @id() @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt()
  name        String
  description String   @default("")
  startPointX Float
  startPointY Float
  startPointZ Float
  endPointX   Float
  endPointY   Float
  endPointZ   Float
}

/// Can be global or local depending on if a project is defined.
model StartingPoint {
  owner       User?    @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId     String?
  id          String   @id() @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt()
  name        String
  description String   @default("")
  startPointX Float
  startPointY Float
  startPointZ Float
  endPointX   Float
  endPointY   Float
  endPointZ   Float
  project     Project? @relation(fields: [projectId], references: [id])
  projectId   String?
}

model Group {
  id                String      @id() @default(cuid())
  createdAt         DateTime    @default(now())
  updatedAt         DateTime    @updatedAt()
  name              String      @unique()
  isAdminGroup      Boolean     @default(false)
  assignedUsers     User[]
  assignedRoles     Role[]
  visibleBaseLayers BaseLayer[]
  visibleProjects   Project[]
  /// This is a glob style matcher that is separated by , because of the limitations of MSSQL in regards to Arrays
  defaultFor        String?
}

model Role {
  id                  String       @id() @default(cuid())
  createdAt           DateTime     @default(now())
  updatedAt           DateTime     @updatedAt()
  assignedGroups      Group[]
  assignedPermissions Permission[]
  name                String       @unique()
  isAdminRole         Boolean      @default(false)
}

/// This is readonly
model Permission {
  id            String   @id() @default(cuid())
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt()
  assignedRoles Role[]
  name          String   @unique()
}

model BaseLayer {
  owner              User?     @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId            String?
  id                 String    @id() @default(cuid())
  createdAt          DateTime  @default(now())
  updatedAt          DateTime  @updatedAt()
  name               String
  description        String    @default("")
  sizeGB             Float
  href               String?
  /// Can be one of 3D-TILES, TERRAIN, IMAGERY, WMS
  type               String
  visibleForGroups   Group[]
  /// Can be one of PENDING | FAILED | COMPLETED
  status             String    @default("COMPLETED")
  progress           Float     @default(0)
  includedInProjects Project[]
}

model ProjectLayer {
  owner              User?         @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId            String?
  id                 String        @id() @default(cuid())
  createdAt          DateTime      @default(now())
  updatedAt          DateTime      @updatedAt()
  name               String
  description        String        @default("")
  sizeGB             Float
  href               String?
  projectFiles       ProjectFile[]
  includedInProjects Project[]     @relation("includedInProjects")
  belongsToProject   Project       @relation(fields: [belongsToProjectId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  belongsToProjectId String
}

model Event {
  owner              User?           @relation(fields: [ownerId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  ownerId            String?
  id                 String          @id() @default(cuid())
  createdAt          DateTime        @default(now())
  updatedAt          DateTime        @updatedAt()
  /// Can be one of MISSING_HOST, ACTIVE, PLANNED, END
  status             String
  title              String          @db.Text()
  startTime          DateTime
  endTime            DateTime
  joinCode           String?
  heartbeatTimestamp DateTime?
  project            Project?        @relation(fields: [projectId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  projectId          String?
  attendees          EventAttendee[]
}

/// This is explicit because of potential role assignments per event
model EventAttendee {
  event   Event  @relation(fields: [eventId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  eventId String
  user    User   @relation(fields: [userId], references: [id])
  userId  String
  /// Can be one of GUEST, MODERATOR
  role    String @default("GUEST")

  @@id([eventId, userId])
}

model Configuration {
  id                              String   @id() @default(cuid())
  createdAt                       DateTime @default(now())
  updatedAt                       DateTime @updatedAt()
  defaultEPSG                     String
  globalStartPointX               Float
  globalStartPointY               Float
  globalStartPointZ               Float
  maxParallelFileConversions      Float
  maxParallelBaseLayerConversions Float
  invitationEmailText             String   @db.Text()
  localProcessorFolder            String
}
